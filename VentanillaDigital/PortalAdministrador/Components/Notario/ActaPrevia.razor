                <p class="text-center resumen_acta-resumen">Resumen de trámite notarial @actaResumen.TramiteId</p>

                <p class="text-center resumen_acta-nombre_notaria">@actaResumen.NotariaNombre</p>

                <p class="text-center resumen_acta-tramite"><strong>@actaResumen.TipoTramiteNombre</strong></p>

                <p class="text-center" style="margin-bottom: 1.22rem;">@actaResumen.TramiteFecha.ToString()</p>
                
                <table class="resumen_acta-tabla">
                    <thead>
                        <tr>
                            <th>Foto</th>
                            <th>Compareciente</th>
                            <th>NUIP</th>
                            <th>Biometria</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < actaResumen.Comparecientes.Count; i++)
                        {
                            var item = actaResumen.Comparecientes[i];
                            <tr>
                                <td class="resumen_acta-tabla-foto"><img src="@item.Foto"></td>
                                <td class="resumen_acta-tabla-nombres">@item.Nombres @item.Apellidos</td>
                                <td>@item.TipoDocumento @item.NumeroDocumento</td>
                                <td>
                                    @if (item.SinBiometria)
                                    {
                                        <span>No tiene biometria debido a @item.MotivoSinBiometria</span>
                                    }
                                    else
                                    {
                                        <span class="resumen_acta-verde"><strong>Hit</strong></span>
                                    }
                                    @*@if(i == 0
                                && (actaResumen.TipoTramiteId == (long)CodigoTipoTramite.DocumentoPrivadoFirmaARuego
                                || (actaResumen.TipoTramiteId == (long)CodigoTipoTramite.DocumentoPrivadoInvidente
                                ){
                                <p>Rogante<p>
                                }else {
                                <p>Rogado<p>
                                }*@
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                
                @if(actaResumen.Estado == "Rechazado"){
                    <p class="resumen_acta-detalle-acto">El trámite ha sido <strong>rechazado</strong></p>
                    var text = ConvertirJson<DatosRechazo>(actaResumen.DatosAdicionales);
                    <p class="resumen_acta-detalle-detalles">Motivo: @text.MotivoRechazo</p>
                }

                @if (!string.IsNullOrWhiteSpace(actaResumen.DatosAdicionales))
                {
                    <p class="resumen_acta-h3">Detalles adicionales del trámite</p>
                    <div style="padding-left: 0.8rem; margin-bottom: 1rem">
                        @switch ((CodigoTipoTramite)actaResumen.TipoTramiteId)
                        {
                            case CodigoTipoTramite.DocumentoPrivado:
                                var text = ConvertirJson<DocumentoPrivadoDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text.ActoNotarial</p>
                                <p class="resumen_acta-detalle-firmantes"><strong>Firmante(s): </strong>@text.Firmantes</p>
                                <p class="resumen_acta-detalle-detalles"><strong>Detalle: </strong>@text.DetalleDeActo</p>
                                break;
                            case CodigoTipoTramite.EscrituraPublica:
                                var text2 = ConvertirJson<EscrituraPublicaDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text2.ActoNotarial</p>
                                <p><strong>Fecha de trámite: </strong>@text2.FechaTramite.ToString()</p>
                                <p><strong>Numero de referencia: </strong>@text2.NumeroReferencia</p>
                                break;
                            case CodigoTipoTramite.DeclaracionExtraProceso:
                                var text3 = ConvertirJson<DeclaracionExtraProcesoDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text3.ActoNotarial</p>
                                <p><strong>Destinatario: </strong>@text3.Destinatario</p>
                                break;
                            case CodigoTipoTramite.PresentacionPersonal:
                                var text4 = ConvertirJson<PresentacionPersonalDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Destinatario: </strong>@text4.Destinatario</p>
                                if(!string.IsNullOrWhiteSpace(@text4.TarjetaProfesional)){
                                    <p class="resumen_acta-detalle-acto"><strong>Tarjeta profesional: </strong>@text4.TarjetaProfesional</p>
                                }
                                break;
                            case CodigoTipoTramite.AutenticacionHuella:
                                <em>No contiene datos adicionales</em>
                                break;
                            case CodigoTipoTramite.InscripcionRegistroCivil:
                                var text6 = ConvertirJson<InscripcionRegistroCivilDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Nombres inscritos: </strong>@text6.NombresApellidosInscritos</p>
                                <p><strong>Registro civil: </strong>@text6.TipoRegistroCivil</p>
                                break;
                            case CodigoTipoTramite.DocumentoPrivadoFirmaARuego:
                                var text7 = ConvertirJson<DocumentoPrivadoFirmaARuegoDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text7.ActoNotarial</p>
                                <p class="resumen_acta-detalle-firmantes"><strong>Firmante(s): </strong>@text7.Firmantes</p>
                                <p class="resumen_acta-detalle-detalles"><strong>Detalle: </strong>@text7.DetalleDeActo</p>
                                break;
                            case CodigoTipoTramite.DocumentoPrivadoInvidente:
                                var text8 = ConvertirJson<DocumentoPrivadoInvidenteDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text8.ActoNotarial</p>
                                <p class="resumen_acta-detalle-firmantes"><strong>Firmante(s): </strong>@text8.Firmantes</p>
                                <p class="resumen_acta-detalle-detalles"><strong>Detalle: </strong>@text8.DetalleDeActo</p>
                                <p class="resumen_acta-detalle-detalles">El compareciente @(@text8.SabeFirmar ? "sabe firmar": "no sabe firmar").</p>
                                break;
                            case CodigoTipoTramite.AutenticacionFirma:
                                var text9 = ConvertirJson<AutenticacionFirmaDTO>(actaResumen.DatosAdicionales);
                                <p class="resumen_acta-detalle-acto"><strong>Acto(s): </strong>@text9.ActoNotarial</p>
                                <p class="resumen_acta-detalle-firmantes"><strong>Firmante(s): </strong>@text9.Firmantes</p>
                                <p class="resumen_acta-detalle-detalles"><strong>Detalle: </strong>@text9.DetalleDeActo</p>
                                break;
                        }
                    </div>
                }
            }
            else
            {
                <p>Cargando...</p>
            }
        </div>
    </div>
</div>
